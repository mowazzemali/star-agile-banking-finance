pipeline {
    agent any
    stages{
        stage('build project'){
            steps{
                git url:'https://github.com/mowazzemali/star-agile-banking-finance'
                sh 'mvn clean package'
              
            }
        }
        stage('Build docker image'){
            steps{
                script{
                    sh 'docker build -t mowazzemali/star-agile-banking-financev1 .'
                    sh 'docker images'
                }
            }
        }
         
        
     stage('Deploy') {
            steps {
                sh 'sudo docker run -itd --name My-first-containe2211 -p 8083:80 mowazzemali/star-agile-banking-finance:v1'
                  
                }
            }
        
    }
}
